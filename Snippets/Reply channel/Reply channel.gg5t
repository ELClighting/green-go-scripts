    var replychan = 0
    for i = 1 to 32
        event channel.input.status[i]
            replychan = event.value == 3 ? event.index : ""
        end
    end
    event ui.draw
        ui.clear()
        ui.fontmode = ui.fontmode.center
        ui.draw.text(replychan)
    end
